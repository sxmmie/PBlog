@model IEnumerable<Post>

@{
}

<h1>Home</h1>

<div class="main-img">
    <span class="title">My Personal Blog</span>
</div>

<div class="container">
    <ul>
        @foreach (var post in Model)
        {
        <li>
            <a asp-controller="Home" asp-action="Post" asp-route-id="@post.Id">
                @post.Title
            </a>
        </li>
        }
    </ul>
</div>
